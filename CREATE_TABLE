CREATE TABLE OBRAS_LITERARIAS
(
ID INT PRIMARY KEY AUTO_INCREMENT,
TITULO CHAR (255),
RESUMO VARCHAR (2000),
AUTOR CHAR (255),
AUTO_PUBLICACAO YEAR,
GENERO CHAR (100),
TEMAS VARCHAR (300),
NUMERO_DE_PAGINAS INT,
SINOPSE VARCHAR (2000),
IMPACTO_CULTURAL CHAR (255),
IDIOMA CHAR (100),
CLASSIFICACAO CHAR (200),
EDITORA CHAR (255),
PERSONAGENS_PRINCIPAIS VARCHAR(300),
PREMIOS INT,
PUBLICO_ALVO CHAR (200),
PAIS_ORIGEM CHAR (200),
PRECO DECIMAL(10,2),
CUSTO_PRODUCAO DECIMAL (10,2)
);

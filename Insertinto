Create table OBRAS_LITERARIAS (
ID INT NOT NULL,
TITULO VARCHAR(255),
AUTOR VARCHAR(255),
ANO_PUBLICACAO YEAR,
GENERO VARCHAR(255),
TEMAS VARCHAR(255),
CONTEXTO_HISTORICO VARCHAR(255),
PERSONAGENS_PRINCIPAIS VARCHAR(255),
RESUMO VARCHAR(2000),
IMPACTO_CULTURAL VARCHAR(255),
CLASSIFICACAO VARCHAR(255),
SINOPSE VARCHAR(2000),
NUM_PAGINAS INT,
PREMIOS INT,
PUBLICO_ALVO VARCHAR(50),
PAIS_ORIGEM VARCHAR(50),
IDIOMA VARCHAR(50),
PRECO DECIMAL(10,2),
CUSTO_PRODUCAO DECIMAL(10,2),
EDITORA VARCHAR (50)
)
